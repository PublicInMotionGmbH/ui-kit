{
  "name": "@talixo/ui-kit",
  "version": "0.1.0",
  "description": "UI Kit for Talixo",
  "main": "index.js",
  "scripts": {
    "create": "node scripts/generatePackage.js",
    "init:yarn": "yarn install",
    "init": "npm install",
    "test": "node ./scripts/runTests.js",
    "test:watch": "npm test -- --watch",
    "bootstrap": "lerna bootstrap --hoist && lerna link",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "build": "node ./scripts/buildPackages.js",
    "compare-own-versions": "node ./scripts/fixLocalVersions.js",
    "compare-dependencies": "node ./scripts/fixDependenciesVersions.js",
    "readme-dependencies": "node ./scripts/fixReadmeVersions.js",
    "gh-pages": "npm run build-storybook -- -o _gh-pages && gh-pages -d _gh-pages && rm -rf _gh-pages",
    "postinstall": "lerna bootstrap --hoist && lerna link && npm run build",
    "prepublishOnly": "npm test",
    "postpublish": "npm run gh-pages",
    "lint": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "author": "Public in Motion GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/PublicInMotionGmbH/ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/PublicInMotionGmbH/ui-kit/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.14",
    "@storybook/addon-info": "^4.0.0-alpha.14",
    "@storybook/addon-links": "^4.0.0-alpha.14",
    "@storybook/addons": "^4.0.0-alpha.14",
    "@storybook/react": "^4.0.0-alpha.14",
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "filesize": "^3.6.1",
    "gh-pages": "^1.1.0",
    "glob": "^7.1.2",
    "glob-loader": "^0.3.0",
    "html-loader": "^0.5.5",
    "husky": "^0.15.0-rc.13",
    "inquirer": "^5.1.0",
    "isbinaryfile": "^3.0.2",
    "jest": "^21.2.1",
    "jest-junit": "^3.6.0",
    "lerna": "^2.9.0",
    "lodash": "^4.17.10",
    "markdown-loader": "^3.0.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sass-loader": "^6.0.7",
    "semver": "^5.5.0",
    "semver-compare-range": "0.0.3",
    "style-loader": "^0.20.3",
    "yargs": "^11.0.0",
    "sinon": "^6.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-textarea-autosize": "^6.1.0"
  }
}
